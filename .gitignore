# Ignore bundler config
/.bundle

# IP Location

/app/assets/**/*.eps

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore configuration files
/config/*.yml
*.sublime*
*.vscode*

# Ignore all logfiles and tempfiles.
/tmp
.DS_Store
*.rdb

#Sass Caching
.sass-cache
*~
# Ignore node modules
/node_modules

npm-debug.lo*
public/system/
public/system/*

# Ignore NPM's default lock file
package-lock.json

# Ignore webpack built node_modules
/app/assets/javascripts/modules

# Ignore duplicate JS files
/app/assets/javascripts/dist/*
/app/assets/stylesheets/dist/*